{
	"plugins": ["plugins/markdown"],
	"templates": {
		"default": {
			"staticFiles": {"paths": ["static"]}
		}
	},
	"opts": {
		"encoding": "utf8",
		"destination": "./docs/",
		"recurse": true
	},
	"source": {
		"include": ["./lib", "./README.md"],
		"exclude": [],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	}
}
